<h1><%= @hello %></h1>
<ul>
    <% @articles.each do |article| %>
    <li>
    <%= article.title %>
    <%= link_to  "Edit", edit_article_path(article) %>
    <%= link_to  "Delete", article_path(article), method: :delete, data: { confirm: "Are your sure?" } %>
    </li>
    <% end %>
</ul>
<%= link_to  "Add new article", new_article_path %>
